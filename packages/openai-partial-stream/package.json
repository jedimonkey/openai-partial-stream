{
    "name": "openai-partial-stream",
    "author": "Yanael Barbier",
    "license": "MIT",
    "private": false,
    "version": "0.3.0",
    "devDependencies": {
        "@types/node": "^20.6.0",
        "typescript": "^5.2.2"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "openai": "^4.6.0",
        "zod": "^3.22.2",
        "zod-to-ts": "^1.2.0"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/**/*.ts --format esm,cjs --dts --clean",
        "dev": "npm run build -- --watch"
    },
    "publishConfig": {
